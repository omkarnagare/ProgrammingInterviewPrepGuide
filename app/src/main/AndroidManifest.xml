            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.nagare.balkrishna.omkar.programminginterviewprepguide.Activity.SettingsActivity"></activity>
    </application>

</manifest>